                                                    <RadioButton IsChecked="{Binding IsSolo, Mode=TwoWay}"/>
                                                </Viewbox>
                                                <Viewbox Grid.Column="2" Height="15"  VerticalAlignment="Top" Margin="0, 3, 0, 0">
                                                    <RadioButton IsChecked="{Binding IsMuted, Mode=TwoWay}"/>
                                                </Viewbox>
                                                <Slider Width="100" Grid.Column="3" VerticalAlignment="Top" Value="100" Style="{StaticResource SliderStyle1}" HorizontalAlignment="Center"/>
                                            </Grid>
                                        </DataTemplate>
                                    </ListView.ItemTemplate>
                                </ListView>
                                <ScrollViewer  Grid.Column="1" HorizontalScrollBarVisibility="Auto" ZoomMode="Disabled">
                                    <ItemsControl  VerticalAlignment="Top" ItemsSource="{Binding OrpheeFile.OrpheeTrackList, Mode=TwoWay}">
                                        <ItemsControl.ItemTemplate>
                                            <DataTemplate>
                                                <StackPanel>
                                                    <ItemsControl Name="ColumnMapItemsControl" ItemsSource="{Binding ColumnMap}">
                                                        <ItemsControl.ItemsPanel>
                                                            <ItemsPanelTemplate>
                                                                <ItemsStackPanel Orientation="Horizontal" Background="DimGray"/>
                                                            </ItemsPanelTemplate>
                                                        </ItemsControl.ItemsPanel>
                                                        <ItemsControl.ItemTemplate>
                                                            <DataTemplate>
                                                                <StackPanel Orientation="Horizontal">
                                                                    <Line X1="0" Y1="0" X2="0" Y2="1" Stretch="Fill"  Stroke="#BDBEC0"/>
                                                                        <Rectangle Fill="{Binding DataContext.UI.TrackColor, ElementName=ColumnMapItemsControl}"  Opacity="{Binding IsRectangleVisible}" Height="35" Width="35"/>
                                                                    <Line X1="0" Y1="0" X2="0" Y2="1" Stretch="Fill" Stroke="#BDBEC0" Margin="0, 0, -1, 0"/>
                                                                </StackPanel>
                                                            </DataTemplate>
                                                        </ItemsControl.ItemTemplate>
                                                    </ItemsControl>
                                                    <Line VerticalAlignment="Bottom" X1="0" Y1="0" X2="150" Y2="0" Stroke="#BDBEC0" Stretch="Fill"/>
                                                </StackPanel>
                                            </DataTemplate>
                                        </ItemsControl.ItemTemplate>
                                    </ItemsControl>
                                </ScrollViewer>
                            </Grid>
                        </ScrollViewer>
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</Page>